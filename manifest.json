{
  "name": "Take a break",
  "description": "Take a break is a producitivity extension to remind you to take breaks regularly.",
  "version": "1.0.0",
  "manifest_version": 3,
  "action":{
    "default_popup": "index.html",
    "default_title": "Take a break"
  },
  "permissions":[
    "alarms",
    "tabs",
    "activeTab",
    "storage"
    
  ],
  "host_permissions":[
    "<all_urls>"
  ],
 
  "background":{
    "service_worker":"./background.js"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
 
  }
}
